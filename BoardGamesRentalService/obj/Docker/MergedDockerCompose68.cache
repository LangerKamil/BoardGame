services:
  angular-docker:
    build:
      context: .
      dockerfile: GamesRental.View/Dockerfile
    ports:
    - mode: ingress
      target: 4200
      published: 4200
      protocol: tcp
  boardgamesrentalservice:
    build:
      context: .
      dockerfile: BoardGamesRentalService/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BoardGamesRentalService
      target: base
    container_name: BoardGamesRentalService_1
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    expose:
    - "8080"
    image: boardgamesrentalservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net5.0/BoardGamesRentalService.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 8080
      published: 8080
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\Env3lope\source\repos\BoardGame\BoardGamesRentalService\BoardGamesRentalService
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Env3lope\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Env3lope\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Env3lope\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files\dotnet\sdk\NuGetFallbackFolder
      target: /root/.nuget/fallbackpackages2
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Env3lope\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Env3lope\source\repos\BoardGame\BoardGamesRentalService\.
      target: /src
      bind:
        create_host_path: true
  migrations:
    build:
      context: .
  sql-server:
    depends_on:
      migrations:
        condition: service_started
    environment:
      ACCEPT_EULA: '"Y"'
    image: mcr.microsoft.com/mssql/server:2019-CU13-ubuntu-20.04
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp